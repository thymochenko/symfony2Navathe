{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Departamento</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Dnome</th>
                <td>{{ entity.dnome }}</td>
            </tr>
            <tr>
                <th>Dnumero</th>
                <td>{{ entity.dnumero }}</td>
            </tr>
            <tr>
                <th>Datainiciogerente</th>
                <td>{{ entity.dataInicioGerente|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('departamento') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('departamento_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
